#include<bits/stdc++.h>
#define pf printf
#define sf scanf
#define z long long int
using namespace std ;
int main()
{
	
	string s;
	z t,v,c,q1,q2,q3,q4,q5;
	
	sf("%lld",&t);
 
	while(t--)
	{
			cin>>s;
				v=c=0;
				q1=q2=q3=q4=q5=0;
			for(int i=0;i<s.length();i++)
			{
				if(s[i]=='a')
					q1++;
				if(s[i]=='e')
					q2++;
				if(s[i]=='i')
					q3++;
				if(s[i]=='o')
					q4++;
				if(s[i]=='u')
					q5++;
			}
 
			v=q1+q2+q3+q4+q5;
			if(v>=5 && q1>=1 && q2>=1 && q3>=1 && q4>=1 && q5>=1)
				cout<<s<<endl;
	}
	return 0;
}  
